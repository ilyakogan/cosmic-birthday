<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.cosmicbirthday">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application android:label="@string/app_name" android:allowBackup="true" android:logo="@drawable/logo"
        android:icon="@drawable/logo"  android:theme="@style/AppTheme">
        <activity android:name=".ui.PeopleActivity" android:parentActivityName=".ui.MainActivity">
        </activity>
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/facebook_app_id"/>
    </application>

</manifest>
